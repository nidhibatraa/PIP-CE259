

string = input() 
swap = []

for i in range(len(string)): 

     if(string[i].islower()): 
         swap.append(string[i].upper()) 
         
     elif(string[i].isupper()): 
         swap.append(string[i].lower()) 
         
     else: 
         swap.append(string[i]) 
string1 = ""
print(string1.join(swap)) 
